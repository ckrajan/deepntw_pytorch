   "source": [
    "import torch\n",
    "\n",
    "x = torch.tensor([1])\n",
    "y = torch.tensor([2])\n",
    "w = torch.tensor([1.], requires_grad=True)\n",
    "\n",
    "# forward pass\n",
    "y_pred = w * x\n",
    "loss = (y_pred - y)**2\n",
    "\n",
    "# backward pass\n",
    "loss.backward()\n",
    "print(w.grad)"
   ]
  }
