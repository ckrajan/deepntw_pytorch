   "source": [
    "import torch\n",
    "\n",
    "x = torch.ones((2,2), requires_grad=True)\n",
    "print(x)\n",
    "\n",
    "y = x+2\n",
    "print(y)\n",
    "\n",
    "z =  y.sum()\n",
    "print(z)\n",
    "\n",
    "z.backward()\n",
    "print(x.grad)\n",
    "\n",
    "z =  y\n",
    "v = torch.tensor(([2., 2.],[2., 2.]), dtype= torch.float32)\n",
    "\n",
    "z.backward(v)\n",
    "print(x.grad)\n",
    "\n",
    "x.requires_grad_(False)\n",
    "print(x)"
   ]
  }
